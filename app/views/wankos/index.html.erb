<h1><%=t('defaults.title.index', item: Wanko.model_name.human)%></h1>      

<table>
  <thead>
    <tr>
    <th><%=Wanko.human_attribute_name(:id)%></th>
      <th><%=Wanko.human_attribute_name(:name)%></th>
      <th><%=Wanko.human_attribute_name(:picture)%></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @wankos.each do |wanko| %>
      <tr>
        <td><%= wanko.id %></td>
        <td><%= wanko.name %></td>
        <td><%= image_tag wanko.picture_url , size: '100x100'%></td>
        <td><%= link_to t('defaults.show'), wanko %></td>
        <td><%= link_to t('defaults.edit'), edit_wanko_path(wanko) %></td>
        <td><%= link_to t('defaults.destroy'), wanko, method: :delete, data: { confirm: t('defaults.message.destroy_confirm', item: Wanko.model_name.human) } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to t('defaults.title.new', item: Wanko.model_name.human), new_wanko_path %>
